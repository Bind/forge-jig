{
  "pipeline": {
    "local": {
      "dependsOn": ["bundle"]
    },
    "bundle": {
      "dependsOn": ["build", "^bundle"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
