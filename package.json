{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "SOL_AST_COMPILER_CACHE=~/.compiler-cache tsdx watch",
    "build": "SOL_AST_COMPILER_CACHE=~/.compiler-cache tsdx build",
    "test": "SOL_AST_COMPILER_CACHE=~/.compiler-cache tsdx test",
    "lint": "SOL_AST_COMPILER_CACHE=~/.compiler-cache tsdx lint",
    "prepare": "SOL_AST_COMPILER_CACHE=~/.compiler-cache tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "forge-fixture",
  "author": "Doug Binder",
  "module": "dist/forge-fixture.esm.js",
  "size-limit": [
    {
      "path": "dist/forge-fixture.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/forge-fixture.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "solc-typed-ast": "^9.0.0"
  }
}
